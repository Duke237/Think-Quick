{
  "name": "think-quick-server",
  "version": "1.0.0",
  "description": "Think Quick Game Server - Family Feud style game",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "seed": "node -e \"require('./src/config/database').connectDB().then(() => require('./src/seeds/seedDB').seedQuestions()).then(() => { console.log('Done'); process.exit(0); }).catch(err => { console.error(err); process.exit(1); })\"",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": ["game", "quiz", "real-time", "socket.io", "family-feud"],
  "author": "Davida Assene, Neba Eric",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "socket.io": "^4.6.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "mongodb-memory-server": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}





